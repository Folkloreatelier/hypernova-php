{
    "name": "folklore/hypernova-php",
    "description": "PHP client for Hypernova. Supports Laravel",
    "keywords": ["hypernova","react", "laravel","javascript","ui","reactjs"],
    "homepage": "http://github.com/Folkloreatelier/hypernova-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@atelierfolklore.ca",
            "homepage": "http://atelierfolklore.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
        "orchestra/database": "3.1.*|3.2.*|3.3.*|3.4.*@dev",
        "orchestra/testbench": "3.1.*|3.2.*|3.3.*|3.4.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0|~5.0",
        "symfony/css-selector": "2.7.*|2.8.*|3.0.*|3.1.*|3.2.*",
        "symfony/dom-crawler": "2.7.*|2.8.*|3.0.*|3.1.*|3.2.*",
        "satooshi/php-coveralls": "^1.0",
        "php-mock/php-mock-phpunit": "^1.1"
    },
    "suggest": {
        "illuminate/support": "To support laravel"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
    "autoload": {
        "classmap": [
            "tests/"
        ],
        "psr-0": {
            "Folklore\\Hypernova\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
